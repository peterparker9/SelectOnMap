<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="2dp"
        android:bottom="2dp"
        android:left="2dp"
        >
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke
                android:width="2dp"
                android:color="#d8d8d8"
                android:dashGap="2dp"
                android:dashWidth="2dp"/>
        </shape>
    </item>
</layer-list>